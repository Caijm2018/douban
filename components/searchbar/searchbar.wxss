.searchbar{
  background-color: #41be57;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}

.search-navigator{
    width: 100%;
    height: 60rpx;
    background-color: #fff;
    border-radius: 10rpx;
    background-image:url(data:image/png;base64,);
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 6%;
}

.search-input-group{
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
}

.search-input-group .search-input{
  width: 100%;
  min-height: 40rpx;
  height: 40rpx;
  font-size: 22rpx;
}